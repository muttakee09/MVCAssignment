<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    assembly="..." namespace="...">

	<class name="Students" table="Students">

		<id name="StudentId" column="id" generator="native" />

		<property name="Age" column="Age" />
		<property name="Gender" column="Gender" />
		<property name="StudentName" column="StudentName" />
		<many-to-one name="MainCourse" column="MainCourse" />
		<many-to-one name="SuplementaryCourse" column="SupplementaryCourse" />

	</class>

	<class name="Courses" table="Courses">

		<id name="Id" column="id">
			<generator class="native"/>
		</id>

		<property name="CourseName" column="CourseName" />
		<property name="CourseCode" column="CourseCode" />
		<property name="CourseCredit" column="CourseCredit" />
		
		<set name="Students" table="Students" inverse="true" lazy="true">
			<key column="MainCourse"/>
			<one-to-many class="Courses" />
		</set>

		<set name="Students" table="Students" inverse="true" lazy="true">
			<key column="SupplementaryCourse"/>
			<one-to-many class="Courses" />
		</set>

	</class>

</hibernate-mapping>