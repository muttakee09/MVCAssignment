@model MVCAssignment.Models.Student

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2>@ViewBag.Title</h2>
<h3>@ViewBag.Message</h3>

<h4>Student details</h4>
<hr />
<dl class="dl-horizontal">
    <dt>
        @Html.DisplayNameFor(model => model.StudentName)
    </dt>
    <dd>
        @Html.DisplayFor(model => model.StudentName)
    </dd>

    <dt>
        @Html.DisplayNameFor(model => model.Age)
    </dt>
    <dd>
        @Html.DisplayFor(model => model.Age)
    </dd>

    <dt>
        @Html.DisplayNameFor(model => model.BloodGroup)
    </dt>
    <dd>
        @Html.DisplayFor(model => model.BloodGroup)
    </dd>

    <dt>
        @Html.DisplayNameFor(model => model.Gender)
    </dt>
    <dd>
        @Html.DisplayFor(model => model.Gender)
    </dd>

    <dt>
        @Html.DisplayNameFor(model => model.MainCourse.CourseName)
    </dt>
    <dd>
        @Html.DisplayFor(model => model.MainCourse.CourseName)
    </dd>

    <dt>
        @Html.DisplayNameFor(model => model.SupplementaryCourse.CourseName)
    </dt>
    <dd>
        @Html.DisplayFor(model => model.SupplementaryCourse.CourseName)
    </dd>
</dl>

